{
  "names": {
    "features": {
      "str": ["Al radius [micron]", "CuO radius [micron]", "Al richness"],
      "latex": ["$r_{Al}, [/mu m]$", "$r_{CuO}, [/mu m]$", "$/phi$"]
    },
    "targets": {
      "str": ["Final Pressure [MPa]", "Combustion Temperature [K]"],
      "latex": ["Final Pressure [MPa]", "Combustion Temperature [K]"]
    }
  },
  "scales": {
    "features": [1e-6, 1e-6, 1],
    "targets": [1e6, 1]
  },
  "params_volume":{
    "compute_volume": false,
    "same_sphere_tol":1e-3,
    "max_depth":3,
    "n_subdivision": 128,
    
    "default": {
      "exp_1": [-1, 4.5e-6],
      "exp_2": [0, 2e-5]
    }
  },
  "params_voronoi": {
    "compute_voronoi": {
      "features": true,
      "features_targets": false
    },

    "tol": 1e-3,
    "isFilter": true
  },

  "experiments": {
    "exp0": {
      "color": "blue",
      "name": "name_exp0 [reg_itr]",
      "path": "PATH/history",
      "scale": "real-inverse"
    },
    "exp1": {
      "color": "red",
      "name": "name_exp1 [reg_itr]",
      "path": "PATH/name.csv",
      "scale": "read"
    }
  }
}